<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.text.Font?>

<?import java.net.URL?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <GridPane fx:id="gameTable" gridLinesVisible="true" layoutX="45.0" layoutY="114.0" prefHeight="243.0" prefWidth="320.0">

    </GridPane>
    <Label fx:id="title" layoutX="33.0" layoutY="51.0" text="The Othello Game">
        <font>
          <Font name="Montserrat Regular" size="38.0" />
        </font>
    </Label>
    <Circle fx:id="blackCir" layoutX="404.0" layoutY="125.0" radius="11.0" stroke="BLACK" strokeType="INSIDE" />
    <Circle fx:id="whiteCir" fill="WHITE" layoutX="404.0" layoutY="155.0" radius="11.0" stroke="BLACK" strokeType="INSIDE" />
    <stylesheets>
        <URL value="@Style.css" />
    </stylesheets>
</AnchorPane>
